{
    "struct": {
        "prefix": "struct",
        "body": [
            "//$1",
            "type ${1} struct {",
            "\t${3}",
            "}"
        ],
        "description": "struct"
    },
    "struct constructor": {
        "prefix": "struct",
        "body": [
            "// New$1 returns new $1.",
            "func New${1:type}(${2}) *$1 {",
            "\t$3",
            "\treturn &$1{$4}",
            "}"
        ],
        "description": "constructor for structure"
    },
    "main": {
        "prefix": "main",
        "body": [
            "func main()  {",
            "\t${1}",
            "}"
        ],
        "description": "main function"
    },
    "func": {
        "prefix": "func",
        "body": [
            "func ${1}() ${2} {",
            "\t${3}",
            "}"
        ],
        "description": "function"
    },
    "func method": {
        "prefix": "func",
        "body": [
            "func (${1}) ${2}() ${3} {",
            "\t${4}",
            "}"
        ],
        "description": "function method for struct"
    },
    "err != nil": {
        "prefix": "iferr",
        "body": "if err != nil {\n\t ${1} \n}",
        "description": "if err != nil return err"
    },
    "err != nil (err)": {
        "prefix": "iferr",
        "body": "if err != nil {\n\t return err \n}",
        "description": "if err != nil return err"
    },
    "err != nil (..., err)": {
        "prefix": "iferr",
        "body": "if err != nil {\n\t return ${1:nil}, err\n}",
        "description": "if err != nil return nil, err"
    },
    "err := ...; err != nil (err)": {
        "prefix": "iferr",
        "body": "if err := ${1}; err != nil {\n\treturn err\n}",
        "description": "if err := ...; err != nil return err"
    },
    "err := ...; err != nil (..., err)": {
        "prefix": "iferr",
        "body": "if err := ${1}; err != nil {\n\treturn ${2:nil}, err\n}",
        "description": "if err := ...; err != nil return nil, err"
    }
}