{
  "name": "Go theme and syntax",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "foreground": "#d8dee9",
    "welcomePage.background": "#212529",
    "activityBar.background": "#373a3e",
    "sideBar.background": "#212529",
    "sideBarSectionHeader.background": "#373a3e",
    "notifications.background": "#373a3e",
    "notificationCenterHeader.background": "#373a3e",
    "panel.background": "#212529",
    "editorWidget.background": "#373a3e",
    "menu.background": "#373a3e",
    "statusBar.background": "#3b88ba",
    "statusBar.noFolderBackground": "#3b88ba",
    "editor.background": "#212529"
  },
  "semanticTokenColors": {
    "namespace": "#c3bfa6",
  },
  "tokenColors": [
    {
      "name": "TODO Comment",
      "scope": "keyword.todo",
      "settings": {
        "foreground": "#67cc63"
      }
    },
    {
      "name": "INFO Comment",
      "scope": [
        "keyword.info",
        "keyword.env.info"
      ],
      "settings": {
        "foreground": "#89b7d5"
      }
    },
    {
      "name": "WARN Comment",
      "scope": "keyword.warn",
      "settings": {
        "foreground": "#e3e56f"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment.line",
        "comment.line.double-slash",
        "comment.block"
      ],
      "settings": {
        "foreground": "#616E88"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.other.comma",
        "punctuation.other.period",
        "punctuation.definition.begin.bracket.curly",
        "punctuation.definition.end.bracket.curly",
        "punctuation.definition.begin.bracket.round",
        "punctuation.definition.end.bracket.round",
        "punctuation.definition.bracket.square",
        "punctuation.definition.dictionary",
        "punctuation.definition.array",
        "valid.illegal.expected-dictionary-separator",
        "punctuation.separator.dictionary.pair",
        "punctuation.separator.dictionary.key-value",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#c79e7e"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.import",
        "keyword.package",
        "keyword.control",
        "keyword.const",
        "keyword.map",
        "keyword.var",
        "keyword.type",
        "keyword.struct",
        "keyword.function",
        "keyword.channel",
        "storage.modifier",
        "keyword.go.mod"
      ],
      "settings": {
        "foreground": "#80d3e8"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.comparison",
        "keyword.operator.assignment",
        "keyword.operator.logical",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#80d3e8"
      }
    },
    {
      "name": "Operators Address",
      "scope": [
        "keyword.operator.address",
        "keyword.operator.channel"
      ],
      "settings": {
        "foreground": "#ff80ed"
      }
    },
    {
      "name": "String Format Placeholder",
      "scope": [
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#e1a05b"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.quoted.double",
        "string.quoted.single",
        "string.quoted.raw",
        "property.value",
        "source.ini"
      ],
      "settings": {
        "foreground": "#84a26e"
      }
    },
    {
      "name": "Local Variable",
      "scope": [
        "variable.other.assignment",
        "variable.other.declaration"
      ],
      "settings": {
        "foreground": "#c2c7d1"
      }
    },
    {
      "name": "Variable Type",
      "scope": [
        "variable.type"
      ],
      "settings": {
        "foreground": "#999869"
      }
    },

    {
      "name": "Function Variable Parameter",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
          "foreground": "#858383",
        //"fontStyle": "bold"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#8da176"
      }
    },
    {
      "name": "[Go] nil",
      "scope": [
        "constant.nil.go"
      ],
      "settings": {
        "foreground": "#a1767e"
      }
    },
    {
      "name": "[Go] Err",
      "scope": [
        "variable.err.go"
      ],
      "settings": {
        "foreground": "#fa8faf"
      }
    },
    {
      "name": "Basic Type",
      "scope": [
        "keyword.int"
      ],
      "settings": {
        "foreground": "#89b7d5"
      }
    },
    {
      "name": "Basic Type & Type Struct",
      "scope": [
        "support.type",
        "entity.name.type",
        "entity.name.class.message",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#cdb1e6"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "entity.name.function.support",
      ],
      "settings": {
        "foreground": "#d6a144"//"#e5c300ce"
      }
    },
    {
      "name": "Key",
      "scope": [
        "support.type.property-name",
        "keyword.other",
        "variable.key",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#d6a144"//"#ccad00"
      }
    },
    {
      "name": "[.proto]",
      "scope": [
        "string.unquoted.proto.package"
      ],
      "settings": {
        "foreground": "#c3bfa6"
      }
    }
  ]
}
